<config>
  <!-- HTTP Server -->
  <allow v='127.0.0.1,0.0.0.0'/>

  <!-- Remote Command Server -->
  <command-allow-no-pass v='127.0.0.1,0.0.0.0'/>

  <!-- Client Control -->
  <fold-anon v='true'/>

  <!-- Slot Control -->
  <power v='%POWER%'/>

  <!-- User Information -->
  <team v='%TEAM%'/>
  <user v='%USER%'/>
  <passkey v='%PASSKEY%'/>

  <!-- Folding Slots -->
  <slot id='0' type='CPU'/>
</config>
